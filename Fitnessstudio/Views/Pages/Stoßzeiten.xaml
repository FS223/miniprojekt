<Page x:Class="Fitnessstudio.Stoßzeiten"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Fitnessstudio"
      xmlns:ScottPlot="clr-namespace:ScottPlot.WPF;assembly=ScottPlot.WPF"
      mc:Ignorable="d" 
      d:DesignHeight="725" d:DesignWidth="1350"
      Title="Stoßzeiten">

    <Grid Background="White">

        <WpfPlot x:Name="Timeplot" />
        <Rectangle HorizontalAlignment="Left" Margin="690,34,0,0" Stroke="Red" Width="1" RenderTransformOrigin="0.5,0.5" Panel.ZIndex="10" Height="645" VerticalAlignment="Top">
            <Rectangle.RenderTransform>
                <TransformGroup>
                    <ScaleTransform ScaleX="-1"/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </Rectangle.RenderTransform>
        </Rectangle>


    </Grid>

</Page>
